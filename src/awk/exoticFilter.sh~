#!/bin/bash

$NF!="-"{
    n=split($NF,a,","); 
    if(n==1)
    {
	n2=split($NF,b,"::"); 
	if(n2==2)
	{
	    split(b[1],b1,":"); 
	    split(b[2],b2,":"); 
	    if (b1[1]!=b2[1] && b1[2]!=b2[2]) || ((b1[2]=="+" && b2[2]=="+" && b1[3]>b2[3]) || ((b1[2]=="-" && b2[2]=="-" && b2[3]>b1[3]))	    
		    print $0
	}
    }
}

